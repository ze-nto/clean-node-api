{
  "name": "clean-api",
  "version": "1.0.0",
  "description": "Node API, with a clean architecture",
  "main": "index.js",
  "scripts": {
    "test": "jest --silent --verbose --colors  --noStackTrace --passWithNoTests",
    "test:unit": "npm test -- --watch -c jestUnitConfig.js",
    "test:integration": "npm test -- --watch -c jestIntegrationConfig.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "keywords": [],
  "author": "zeneto",
  "license": "MIT",
  "devDependencies": {
    "husky": "^7.0.2",
    "jest": "^27.2.5",
    "lint-staged": "^11.2.3",
    "standard": "^16.0.4"
  },
  "dependencies": {
    "validator": "^13.6.0"
  }
}
